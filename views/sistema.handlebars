<div class="container">
  <div class="row">
    {{#each clientes}} 
    <div class="col">
      <div class="card" style="width: 18rem;">
        <div class="card-body">
            <p class="card-title">Razão Social: {{razao}}</p>
            <p class="card-text">Capital: {{capital}}</p>
            <p class="card-title">Responsável da Empresa: {{nome}}</p>
            <p class="card-text">Email: {{email}}</p>
            <p class="card-title">Telefone: {{telefone}}</p>
        </div>
      </div>
    </div>
    {{/each}}
  </div>
</div>